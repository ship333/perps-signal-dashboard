# Local development environment for Hyperliquid Signals
# Keep lightweight and runnable without DB/Redis/Alchemy

# System
LOG_LEVEL=INFO
UPDATE_INTERVAL=5
CACHE_TTL=30
MAX_PAIRS_TO_TRACK=6
QUANT_PROVIDER=local
EVENT_DRIVEN_COMPUTE=1

# Providers (use Hyperliquid primary; leave Alchemy off unless configured)
ENABLE_SYNTHETIC_PRICES=1
SYNTHETIC_VOLATILITY=0.005
ALCHEMY_WSS_URL=
ALCHEMY_HTTP_URL=
ALCHEMY_POOLS_JSON=

# Local services (safe defaults; app degrades gracefully if unavailable)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/hliq_signals
REDIS_URL=redis://localhost:6379/0
